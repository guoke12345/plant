<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="pojo.Pest" table="pest">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity"></generator>
        </id>
         <property name="name" type="java.lang.String">
            <column name="name" length="50" />
        </property>
         <property name="othername" type="java.lang.String">
            <column name="othername" length="50" />
        </property>
        <property name="host" type="java.lang.String">
            <column name="host" length="50" />
        </property>
        <property name="location" type="java.lang.String">
            <column name="location" length="255" />
        </property>
        <property name="chara" type="java.lang.String">
            <column name="chara" length="500" />
        </property>
        <property name="aspect" type="java.lang.String">
            <column name="aspect" length="500" />
        </property>
         <property name="habit" type="java.lang.String">
            <column name="habit" length="500" />
        </property>
         <property name="img" type="java.lang.String">
            <column name="img" length="50" />
        </property>
        <property name="measure" type="java.lang.String">
            <column name="measure" length="500" />
        </property>
         <property name="time" type="java.lang.String">
            <column name="time"/>
        </property>
    </class>
</hibernate-mapping>